    <br>&nbsp;&nbsp;&nbsp;&nbsp;o http://api.coronastatistics.live/timeline
    <br>&nbsp;&nbsp;&nbsp;&nbsp;o http://api.coronastatistics.live/timeline/global
    <br>&nbsp;&nbsp;&nbsp;&nbsp;o http://api.coronastatistics.live/timeline/{country_name}
